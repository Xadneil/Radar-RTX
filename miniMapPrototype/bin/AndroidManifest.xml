<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.devilishro.minimap"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="net.devilishro.minimap.Minimap"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.devilishro.minimap.EventActivity"
            android:label="@string/title_activity_event" >
        </activity>
        <activity
            android:name="net.devilishro.minimap.EventJoinActivity"
            android:label="@string/title_activity_event_join" >
        </activity>
        <activity
            android:name="net.devilishro.minimap.MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity android:name="net.devilishro.minimap.EventAdd" >
        </activity>
        <activity
            android:name="net.devilishro.minimap.Note_acti"
            android:label="@string/title_activity_note_acti" >
        </activity>
        <activity
            android:name="net.devilishro.minimap.Player_state"
            android:label="@string/title_activity_player_state" >
        </activity>
        <activity
            android:name="net.devilishro.minimap.Replay"
            android:label="@string/title_activity_replay" >
        </activity>
    </application>

</manifest>
